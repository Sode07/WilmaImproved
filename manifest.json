{
  "manifest_version": 3,
  "name": "WilmaImproved",
  "version": "0.1.0",

    "permissions": [
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://turku.inschool.fi/*"],
      "js": ["Content.js"]
    }
  ]
}
